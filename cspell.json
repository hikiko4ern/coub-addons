{
	"version": "0.2",
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/master/cspell.schema.json",
	"import": [
		"@cspell/dict-en-gb/cspell-ext.json",
		"@cspell/dict-ru_ru/cspell-ext.json"
	],
	"language": "en,ru",
	"enableFiletypes": [
		"fluent"
	],
	"dictionaries": [
		"css",
		"fonts",
		"node",
		"npm",
		"softwareTerms",
		"typescript",
		"en-gb"
	],
	"minWordLength": 3,
	"flagWords": [],
	"cache": {
		"useCache": true
	},
	"validateDirectives": true,
	"useGitignore": true,
	"words": [
		"Corepack",
		"coub",
		"coubs",
		"runtimes",
		"дизлайкнутых",
		"коуб",
		"коубов",
		"коубу",
		"коубы",
		"лейблы",
		"пропатчить",
		"стора",
		"сторах",
		"фейковых",
		"форки"
	],
	"ignoreWords": [
		"flexsearch",
		"uniqueEverseen",
		"cssnano",
		"dprint",
		"heroicons",
		"ifilter",
		"itertools",
		"nextui",
		"plasmo",
		"preact",
		"prefresh",
		"superstruct",
		"tailwindcss",
		"biomejs",
		"bunx",
		"clsx",
		"elems",
		"fmt",
		"julr",
		"hikiko4ern",
		"Wouter",
		"wxt",
		"XMLHttpRequest",
		"xpinstall",
		"BCP47"
	],
	"ignoreRegExpList": [
		// imports
		"/^import( .*? from)? '[^']*?';$/m",
		// Markdown ToC elements
		"/^- \\[[^\\]]*\\]\\((#[^)]*)\\)$/m",
		// deps
		"/@fluent/\\w+/"
	],
	"ignorePaths": [
		"biome.json",
		"bun.lockb",
		"./cspell.json",
		"packages/@nextui-org"
	]
}
