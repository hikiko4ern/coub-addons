{
	"version": "0.2",
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/master/cspell.schema.json",
	"import": [
		"@cspell/dict-en-gb/cspell-ext.json",
		"@cspell/dict-ru_ru/cspell-ext.json"
	],
	"language": "en,ru",
	"enableFiletypes": [
		"fluent"
	],
	"dictionaries": [
		"css",
		"fonts",
		"node",
		"npm",
		"softwareTerms",
		"typescript",
		"rust",
		"en-gb"
	],
	"minWordLength": 3,
	"flagWords": [],
	"cache": {
		"useCache": true
	},
	"validateDirectives": true,
	"useGitignore": true,
	"words": [
		"AMO",
		"Binaryen",
		"cmds",
		"codeberg",
		"coub",
		"coubs",
		"disqus",
		"grayscaled",
		"JST",
		"jwt",
		"pipefail",
		"recoub",
		"recoubs",
		"runtimes",
		"rustup",
		"бэкапы",
		"версионирования",
		"дизлайкнутых",
		"коуб",
		"коуба",
		"коубе",
		"коубов",
		"коубу",
		"коубы",
		"лейблы",
		"пропатчить",
		"рекоубов",
		"рекоубы",
		"репосты",
		"стора",
		"сторах",
		"фейковых",
		"форки"
	],
	"ignoreWords": [
		"Asyncify",
		"ayu",
		"BCP47",
		"bindgen",
		"binstall",
		"biomejs",
		"cdylib",
		"clientside",
		"clsx",
		"codegen",
		"codemirror",
		"Corepack",
		"cssnano",
		"ddietr",
		"dprint",
		"elems",
		"execa",
		"flatmap",
		"flexsearch",
		"fmt",
		"fnm",
		"follow-btn",
		"gfm",
		"gql",
		"hastscript",
		"heroicons",
		"hikiko4ern",
		"ifilter",
		"itertools",
		"julr",
		"justfile",
		"keymap",
		"lefthook",
		"lezer",
		"lightningcss",
		"listr",
		"mdast",
		"micromark",
		"MSRV",
		"nextui",
		"pex",
		"picocolors",
		"preact",
		"prefresh",
		"postcssrc",
		"postversion",
		"rehype",
		"rlib",
		"shiki",
		"shikijs",
		"superstruct",
		"streamsearch",
		"tailwindcss",
		"Taskfile",
		"tempy",
		"thememirror",
		"toastify",
		"tsify",
		"uiw",
		"uniqueEverseen",
		"Vec",
		"wasm_bindgen",
		"Wouter",
		"wsi",
		"wxt",
		"xast",
		"XMLHttpRequest",
		"xpinstall"
	],
	"ignoreRegExpList": [
		// imports
		"/^import( .*? from)? '[^']*?';$/m",
		// Markdown ToC elements
		"/^- \\[[^\\]]*\\]\\((#[^)]*)\\)$/m",
		// deps
		"/@fluent/\\w+/",
		// radicle ID
		"rad:[a-z0-9]+"
	],
	"ignorePaths": [
		"/.gitattributes",
		"/.gitignore",
		"/biome.json",
		"/cspell.json",
		"/pnpm-lock.yaml",
		"/src/gql",
		"/patches"
	]
}
