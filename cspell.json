{
	"version": "0.2",
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/master/cspell.schema.json",
	"import": [
		"@cspell/dict-en-gb/cspell-ext.json",
		"@cspell/dict-ru_ru/cspell-ext.json"
	],
	"language": "en,ru",
	"enableFiletypes": [
		"fluent"
	],
	"dictionaries": [
		"css",
		"fonts",
		"node",
		"npm",
		"softwareTerms",
		"typescript",
		"rust",
		"en-gb"
	],
	"minWordLength": 3,
	"flagWords": [],
	"cache": {
		"useCache": true
	},
	"validateDirectives": true,
	"useGitignore": true,
	"words": [
		"Binaryen",
		"cmds",
		"coub",
		"coubs",
		"pipefail",
		"runtimes",
		"дизлайкнутых",
		"коуб",
		"коубов",
		"коубу",
		"коубы",
		"лейблы",
		"пропатчить",
		"стора",
		"сторах",
		"фейковых",
		"форки"
	],
	"ignoreWords": [
		"Asyncify",
		"BCP47",
		"bindgen",
		"biomejs",
		"cdylib",
		"clsx",
		"codegen",
		"codemirror",
		"Corepack",
		"cssnano",
		"ddietr",
		"dprint",
		"elems",
		"execa",
		"flatmap",
		"flexsearch",
		"fmt",
		"follow-btn",
		"heroicons",
		"hikiko4ern",
		"ifilter",
		"itertools",
		"julr",
		"justfile",
		"keymap",
		"lezer",
		"lightningcss",
		"MSRV",
		"nextui",
		"plasmo",
		"preact",
		"prefresh",
		"rlib",
		"superstruct",
		"tailwindcss",
		"Taskfile",
		"toastify",
		"tsify",
		"uiw",
		"uniqueEverseen",
		"Vec",
		"wasm_bindgen",
		"Wouter",
		"wxt",
		"XMLHttpRequest",
		"xpinstall"
	],
	"ignoreRegExpList": [
		// imports
		"/^import( .*? from)? '[^']*?';$/m",
		// Markdown ToC elements
		"/^- \\[[^\\]]*\\]\\((#[^)]*)\\)$/m",
		// deps
		"/@fluent/\\w+/"
	],
	"ignorePaths": [
		"biome.json",
		"cspell.json",
		"pnpm-lock.yaml",
		"patches",
		"packages/@nextui-org/react-rsc-utils"
	]
}
