// https://dprint.dev/config/
{
	"incremental": true,
	"lineWidth": 100,
	"useTabs": true,
	"indentWidth": 2,
	"newLineKind": "lf",
	"json": {},
	"markdown": {},
	"toml": {},
	"includes": ["**/*.{json,jsonc,json5,md,toml}"],
	"excludes": [
		"**/.output",
		"**/.wxt",
		"**/node_modules",
		"**/LICENSE",
		"**/LICENSE-*",
		"**/CHANGELOG.md",
		"**/src/gql",
		"**/docs/release-notes",
		"**/blockedChannels.json"
	],
	"plugins": [
		"https://plugins.dprint.dev/json-0.19.3.wasm",
		"https://plugins.dprint.dev/markdown-0.17.8.wasm",
		"https://plugins.dprint.dev/toml-0.6.3.wasm"
	]
}
