// https://dprint.dev/config/
{
	"incremental": true,
	"lineWidth": 100,
	"useTabs": true,
	"indentWidth": 2,
	"newLineKind": "lf",
	"json": {},
	"markdown": {},
	"toml": {},
	"includes": ["**/*.{ts,tsx,cts,mts,js,jsx,cjs,mjs,json,jsonc,json5,md,toml}"],
	"excludes": [
		"**/.output",
		"**/.wxt",
		"**/node_modules",
		"**/LICENSE",
		"**/LICENSE-*",
		"**/CHANGELOG.md",
		"**/packages/@nextui-org/react-rsc-utils",
		"**/blockedChannels.json"
	],
	"plugins": [
		"https://plugins.dprint.dev/json-0.19.2.wasm",
		"https://plugins.dprint.dev/markdown-0.16.4.wasm",
		"https://plugins.dprint.dev/toml-0.6.1.wasm"
	]
}
