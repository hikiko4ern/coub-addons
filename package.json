		"dprint": "^0.45.0",
		"husky": "^9.0.10",
		"prettier": "^3.2.5"
	},
	"overrides": {
		"@nextui-org/react-rsc-utils": "./packages/@nextui-org/react-rsc-utils"
	},
	"repository": "https://github.com/hikiko4ern/coub-addons.git",
	"author": "hikiko4ern",
	"license": "Unlicense",
	"private": true,
	"packageManager": "yarn@1.22.21+sha256.dbed5b7e10c552ba0e1a545c948d5473bc6c5a28ce22a8fd27e493e3e5eb6370"
