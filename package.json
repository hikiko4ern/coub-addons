{
	"name": "coub-addons",
	"description": "Coub.com additional features",
	"version": "0.1.2",
	"type": "module",
	"scripts": {
		"prepare": "husky",
		"postinstall": "run-s postinstall:patch postinstall:wxt",
		"postinstall:patch": "patch-package",
		"postinstall:wxt": "wxt prepare",
		"dev": "wxt",
		"dev:ff": "wxt -b firefox",
		"build": "wxt build",
		"build:ff": "wxt build -b firefox",
		"zip": "wxt zip",
		"zip:ff": "wxt zip -b firefox",
		"lint": "run-p \"lint:*\"",
		"lint:biome": "biome check --apply --error-on-warnings .",
		"lint:dprint": "dprint fmt",
		"lint:prettier": "prettier -w .",
		"lint:type-check": "tsc --noEmit",
		"lint:spell-check": "cspell lint --no-progress --no-summary ."
	},
	"dependencies": {
		"@fluent/bundle": "^0.18.0",
		"@fluent/dom": "^0.9.0",
		"@fluent/langneg": "^0.7.0",
		"@fluent/react": "^0.15.2",
		"@fluent/sequence": "^0.8.0",
		"@heroicons/react": "^2.1.1",
		"@julr/vite-plugin-validate-env": "^1.0.1",
		"@nextui-org/accordion": "^2.0.28",
		"@nextui-org/button": "^2.0.26",
		"@nextui-org/card": "^2.0.24",
		"@nextui-org/code": "^2.0.24",
		"@nextui-org/input": "^2.1.16",
		"@nextui-org/link": "^2.0.26",
		"@nextui-org/modal": "^2.0.28",
		"@nextui-org/navbar": "^2.0.27",
		"@nextui-org/pagination": "^2.0.26",
		"@nextui-org/select": "^2.1.20",
		"@nextui-org/snippet": "^2.0.30",
		"@nextui-org/system": "^2.0.15",
		"@nextui-org/table": "^2.0.28",
		"@nextui-org/theme": "^2.1.17",
		"@nextui-org/tooltip": "^2.0.29",
		"@preact/preset-vite": "^2.8.1",
		"@preact/signals": "^1.2.2",
		"@prefresh/vite": "^2.4.5",
		"@types/bun": "^1.0.4",
		"autoprefixer": "^10.4.17",
		"clsx": "^2.1.0",
		"cssnano": "^6.0.3",
		"dotenv-flow": "^4.1.0",
		"flexsearch": "^0.7.43",
		"framer-motion": "^11.0.3",
		"indent-string": "^5.0.0",
		"indexed-iterable": "^1.0.2",
		"itertools": "^2.2.3",
		"nanoid": "^5.0.5",
		"npm-run-all2": "^6.1.2",
		"patch-package": "^8.0.0",
		"postcss": "^8.4.33",
		"preact": "^10.19.3",
		"radix3": "^1.1.0",
		"react-helmet-async": "^2.0.4",
		"react-toastify": "^10.0.4",
		"sass": "^1.70.0",
		"superstruct": "^1.0.3",
		"tailwindcss": "^3.4.1",
		"type-fest": "^4.10.2",
		"typed-query-selector": "^2.11.0",
		"typescript": "5.4.0-beta",
		"use-debounce": "^10.0.0",
		"vite-plugin-checker": "^0.6.4",
		"vite-plugin-sass-dts": "^1.3.17",
		"wouter-preact": "^3.0.0",
